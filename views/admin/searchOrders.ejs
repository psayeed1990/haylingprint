<div id="cart-page">
    <% if(typeof orders != 'undefined') {%>
    <h3>Search by <%= phone %></h3>
    <%for(var i = 0; i < orders.length; i++) { %>
    <% if(orders[i].completed === false){ %>
    <div id="cart">

        <a href="/admin/orders/<%= orders[i].id %>">
            <p style="width: max-content; padding: 10px; margin: 5px auto; border: 1px solid #eee;">
                <%= orders[i].id %> by <%= orders[i].user.name %> <span style="color: #afdfd8;"
                    id="processing">New</span></p>
        </a>



    </div>
    <% } %>

    <% if(orders[i].completed === true){ %>
    <div id="cart">
    
        <a href="/admin/orders/<%= orders[i].id %>">
            <p style="width: max-content; padding: 10px; margin: 5px auto; border: 1px solid #eee;">
                <%= orders[i].id %> by <%= orders[i].user.name %> <span style="color: #afdfd8;" id="processing">Completed</span>
            </p>
        </a>
    
    
    
    </div>
    <% } %>

    <%}%>
    <% } %>
    <br>
    <br>
    <!-- <a class="button" href="/orders/completed">Completed orders</a> -->

</div>